<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>iddriver</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>fechaalta</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>useralta</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>fechamodificacion</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>usermodificacion</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>activo</name>
        <type>boolean</type>
        <default>true</default>
    </column>
    <column>
        <name>fechabaja</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>userbaja</name>
        <type>character varying(50)</type>
    </column>

    <column>
        <name>idemployee</name>
        <type>integer</type>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>observaciones</name>
        <type>text</type>
    </column>
    <constraint>
        <name>driver_pkey</name>
        <type>PRIMARY KEY (iddriver)</type>
    </constraint>
    <constraint>
        <name>ca_driver_employees</name>
        <type>FOREIGN KEY (idemployee) REFERENCES employees (idemployee) ON DELETE RESTRICT ON UPDATE RESTRICT</type>
    </constraint>
    <constraint>
        <name>uniq_idemp_employees</name>
        <type>UNIQUE (idemployee)</type>
    </constraint>
</table>

