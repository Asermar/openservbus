<?xml version="1.0" encoding="UTF-8"?>
<view>
    <columns>
        <column name="Id" display="none" order="100">
            <widget type="text" fieldname="idemployee_type" />
        </column>
        <column name="Nombre" order="120">
            <widget type="text" fieldname="nombre" />
        </column>
        <column name="Obligar-F.Caducidad" display="center" order="150">
            <widget type="checkbox" fieldname="fechacaducidad_obligarla"/>
        </column>
        <column name="Activo" display="center" order="150">
            <widget type="checkbox" fieldname="activo"/>
        </column>
        
        <column name="fechaalta" title="Alta" display="right" order="160">
            <widget type="datetime" fieldname="fechaalta"/>
        </column>
    </columns>
    
    <rows>
        <row type="status">
            <option color="success" fieldname="activo">1</option>
            <option color="warning" fieldname="activo">0</option>
        </row>
    </rows>    
</view>